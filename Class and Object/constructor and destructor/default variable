///without initialize values or variables default constructor

#include <iostream>
#include<conio.h>
#include<stdio.h>
using namespace std;
class Example1
{
    public:
            Example1()
    {
            cout << "Default constructor First called" << endl;
    }
    ~Example1()
     {
        cout << "Destructor Called last After main " << endl;
     }
};

int main()
{
    cout << "We are in main" << endl;
    Example1 obj;

    cout << "We Are Inside Of main "<< endl;
    cout << "press any key to call destructor"<< endl;
    getch();
    return 0;
}

